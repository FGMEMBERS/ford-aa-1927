<?xml version="1.0"?>

<PropertyList>

 <path>RPM.ac</path>

    <params>
        <engine>
            <rpm>engines/engine[2]/rpm</rpm>
        </engine>
    </params>


 <animation> 
  <type>rotate</type>
  <object-name>PointerCentre</object-name>
  <object-name>PointerTip</object-name>
  <property alias="/params/engine/rpm"/>
  <interpolation>
   <entry><ind>0</ind><dep>15</dep></entry>
   <entry><ind>3000</ind><dep>257</dep></entry>
  </interpolation>
  <axis> 
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>  
  </center>
 </animation>         
 
</PropertyList>
